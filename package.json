  "description": "This app has several tools for discord. Mainly related to discord bots and discord API",
  "main": "index.js",
  "build": {
    "appId": "discordtoolsapp",
    "productName": "Discord Tools",
    "win": {
      "icon": "./logos/logo.ico"
